	measure '% Feminino' = DIVIDE([TotalFem], [TotalFunc], 0)
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: fd1104e6-81d2-40cf-a1c7-72b9efb86708

	measure SalarioMedio = AVERAGE(DatasetRH[Salario_Mensal])
