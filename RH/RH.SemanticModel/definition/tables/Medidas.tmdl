	measure TotalFuncPromover = CALCULATE([TotalFunc], DatasetRH[StatusPromo] = "Considerar Promoção")
		formatString: 0
		lineageTag: 3c263cdb-ffec-481a-a5a9-b727bb1fb971

	measure TotalFuncNaoPromover = CALCULATE([TotalFunc], DatasetRH[StatusPromo] = "Não Considerar Promoção")
		formatString: 0
		lineageTag: 98fc916f-6723-49b0-8365-297bc125f3d0

	measure '% Promover' = DIVIDE([TotalFuncPromover], [TotalFunc])
		lineageTag: 91a9c9a0-66b9-4ca6-9ac3-868f0577fe91

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure '% Não Promover' = DIVIDE([TotalFuncNaoPromover], [TotalFunc])
		lineageTag: 58364f97-da44-479f-afb9-9e499bb1b213

		annotation PBI_FormatHint = {"isGeneralNumber":true}

