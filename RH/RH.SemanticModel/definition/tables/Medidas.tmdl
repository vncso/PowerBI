table Medidas
	lineageTag: dae65c67-c492-48e6-bc92-7720c9a58f47

	measure TotalFunc = COUNTROWS('DatasetRH')
		formatString: 0
		lineageTag: d32e7cd7-39a4-4fca-8f3d-94a78b627165

	measure TotalFem = CALCULATE([TotalFunc], DatasetRH[Genero] = "Feminino")
		formatString: 0
		lineageTag: c423db9e-80c4-470e-8d41-cbebed828856

	measure TotalMas = CALCULATE([TotalFunc], DatasetRH[Genero] = "Masculino")
		formatString: 0
		lineageTag: 8825d39c-9ead-428b-ac75-b0495614d1a3

	measure '% Masculino' = DIVIDE([TotalMas], [TotalFunc], 0)
